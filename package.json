{
  "name": "routeros-management-sdk",
  "description": "Complete RouterOS Management SDK with TypeScript support, REST API, Web Interface, and CLI tools for MikroTik devices",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/luismasuarez/routeros-management-sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/luismasuarez/routeros-management-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/luismasuarez/routeros-management-sdk/issues"
  },
  "type": "module",
  "scripts": {
